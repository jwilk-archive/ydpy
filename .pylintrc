[MASTER]
extension-pkg-whitelist = lxml
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-continuation,
    invalid-name,
    protected-access,
    too-few-public-methods,

[BASIC]
no-docstring-rgx = .*

[REPORTS]
reports = no
score = no
msg-template = {path}:{line}: {C}: {symbol} [{obj}] {msg}

[FORMAT]
expected-line-ending-format = LF

